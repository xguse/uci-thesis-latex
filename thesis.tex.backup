\newcommand\synopsis[1]{%
        %\bigskip
        \begin{center}%
                \fbox{
					\begin{minipage}[c]{.95\linewidth}
					\vskip1ex \hskip1ex \textbf{Synopsis:} \textcolor{black}{#1}
