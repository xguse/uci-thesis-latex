journal = {Bioinformatics (Oxford, England)},
keywords = {Computational Biology,Computational Biology: methods,Humans,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Software},
month = sep,
number = {17},
pages = {2283--5},
