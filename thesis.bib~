@article{Young2005,
author = {Young, Jason A and Fivelman, Quinton L and Blair, Peter L and De, Patricia and Le, Karine G and Zhou, Yingyao and Carucci, Daniel J and Baker, David A and Winzeler, Elizabeth A},
