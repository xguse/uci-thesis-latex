@article{Adelman2004,
abstract = {The Class II transposable element, piggyBac, was used to transform the yellow fever mosquito, Aedes aegypti. In two transformed lines only 15-30\% of progeny inherited the transgene, with these individuals displaying mosaic expression of the EGFP marker gene. Southern analyses, gene amplification of genomic DNA, and plasmid rescue experiments provided evidence that these lines contained a high copy number of piggyBac transformation constructs and that much of this DNA consisted of both donor and helper plasmids. A detailed analysis of one line showed that the majority of piggyBac sequences were unit-length donor or helper plasmids arranged in a large tandem array that could be lost en masse in a single generation. Despite the presence of a transposase source and many intact donor elements, no conservative (cut and paste) transposition of piggyBac was observed in these lines. These results reveal one possible outcome of uncontrolled and/or unexpected recombination in this mosquito, and support the conclusion that further investigation is necessary before transposable elements such as piggyBac can be used as genetic drive mechanisms to move pathogen-resistance genes into mosquito populations.},
author = {Adelman, Zach N and Jasinskiene, Nijole and Vally, K J M and Peek, Corrie and Travanty, Emily A and Olson, Ken E and Brown, Susan E and Stephens, Janice L and Knudson, Dennis L and Coates, Craig J and James, Anthony A},
issn = {0962-8819},
journal = {Transgenic research},
keywords = {Aedes,Aedes: genetics,Animals,Baculoviridae,Baculoviridae: genetics,DNA Transposable Elements,DNA Transposable Elements: genetics,Drosophila melanogaster,Drosophila melanogaster: genetics,Fluorescence,Genes,Genetic,Genetically Modified,Green Fluorescent Proteins,Green Fluorescent Proteins: genetics,Green Fluorescent Proteins: metabolism,In Situ Hybridization,Insect,Insertional,Mutagenesis,Promoter Regions,Transformation},
month = oct,
number = {5},
pages = {411--25},
pmid = {15587266},
title = {{Formation and loss of large, unstable tandem arrays of the piggyBac transposable element in the yellow fever mosquito, Aedes aegypti.}},
