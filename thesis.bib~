title = {{Nature, nurture and evolution of intra-species variation in mosquito arbovirus transmission competence.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3564141\&tool=pmcentrez\&rendertype=abstract},
volume = {10},
year = {2013}
