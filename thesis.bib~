@article{Wu2008b,
author = {Wu, Jing and Xie, J U N},
doi = {10.1089/cmb.2008.0024},
journal = {Journal of Computational Biology},
keywords = {bayesian inference,cis-regulatory module,factor binding site,hidden markov model,transcription},
number = {3},
pages = {279--290},
title = {{Computation-Based Discovery of Cis-Regulatory Modules by Hidden Markov Model}},
volume = {15},
year = {2008}
