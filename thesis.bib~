month = jan,
pages = {15},
pmid = {19695103},
title = {{Manipulating insulin signaling to enhance mosquito reproduction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2736915\&tool=pmcentrez\&rendertype=abstract},
volume = {9},
