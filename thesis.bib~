@article{Franz2006,
author = {Franz, Alexander W E and Sanchez-vargas, Irma and Adelman, Zach N and Blair, Carol D and Beaty, Barry J and James, Anthony A and Olson, Ken E},
title = {{Engineering RNA interference-based resistance to dengue virus type 2 in genetically modified Aedes aegypti}},
