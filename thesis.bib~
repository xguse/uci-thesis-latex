@article{Quinlan2010,
author = {Quinlan, Aaron R and Hall, Ira M},
journal = {Methods},
pages = {1--2},
title = {{BEDTools : A flexible suite of utilities for comparing genomic features 2 FEATURES AND METHODS}},
year = {2010}
