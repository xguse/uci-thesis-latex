issn = {1544-6115},
journal = {Statistical applications in genetics and molecular biology},
keywords = {abstract,and phrases,detection plays an important,exact,log-odds score,position,position-weight matrix,power,profile,pssm,pwm,role,significance,specific score matrix,test,tfbs,transcription factor binding site,transfac},
