month = jan,
pages = {437},
pmid = {17996093},
title = {{Identification of tissue-specific cis-regulatory modules based on interactions between transcription factors.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2194798\&tool=pmcentrez\&rendertype=abstract},
