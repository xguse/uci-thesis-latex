month = jul,
number = {Web Server issue},
pages = {W555--9},
pmid = {16845069},
title = {{Stubb: a program for discovery and analysis of cis-regulatory modules.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1538799\&tool=pmcentrez\&rendertype=abstract},
volume = {34},
year = {2006}
