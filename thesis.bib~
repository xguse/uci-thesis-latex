@article{Dickinson2012,
abstract = {Abstract Public health programs that control mosquitoes and other disease vectors have the added benefit of reducing residents' exposure to pest insects. We surveyed homeowners in Madison, Wisconsin, and used an economic valuation method, stated-choice experiments, to measure willingness to pay (WTP) for control of West Nile virus (WNV)-transmitting and nuisance mosquitoes under current and increased levels of WNV risk. Under current WNV risk levels (approximately 1 in 250,000), the average Madison survey respondent was not willing to pay for programs that targeted West Nile-transmitting mosquitoes only (WTP=-\$21, 95\% [CI -\$63, \$20]), while WTP for a reduction in nuisance mosquitoes was substantial (WTP=\$147, 95\% [CI \$109, \$186]). As the risk of WNV was increased, WTP for control of disease-carrying mosquitoes also increased (WTP=\$158; 95\% CI [\$111, \$206] at the highest risk level), but WTP for nuisance control remained high (WTP=\$108; 95\% CI [\$78, \$138]). Among homeowners in our sample, the "nuisance factor" was more important than the "disease factor" in terms of respondents' demand for mosquito control.},
author = {Dickinson, Katherine and Paskewitz, Susan},
doi = {10.1089/vbz.2011.0810},
file = {:home/gus/Dropbox/MendeleyLitWatch/vbz\%2E2011\%2E0810.pdf:pdf},
issn = {1557-7759},
journal = {Vector borne and zoonotic diseases (Larchmont, N.Y.)},
keywords = {culex,mosquito control,mosquitoes,west nile virus},
month = oct,
number = {10},
pages = {886--92},
pmid = {22651384},
title = {{Willingness to pay for mosquito control: how important is west nile virus risk compared to the nuisance of mosquitoes?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22651384},
volume = {12},
year = {2012}
}
