month = jan,
pages = {216},
pmid = {20356352},
title = {{Transcriptome response to pollutants and insecticides in the dengue vector Aedes aegypti using next-generation sequencing technology.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2867825\&tool=pmcentrez\&rendertype=abstract},
volume = {11},
