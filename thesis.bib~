title = {{Stage- and cell-specific expression of ecdysone receptors and ecdysone-induced transcription factors during midgut remodeling in the yellow fever mosquito, Aedes aegypti.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17074360},
volume = {53},
