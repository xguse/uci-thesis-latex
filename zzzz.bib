pages = {631--9},
pmid = {14986924},
title = {{Intraspecific DNA variation in nuclear genes of the mosquito Aedes aegypti.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14986924},
volume = {12},
year = {2003}
}
