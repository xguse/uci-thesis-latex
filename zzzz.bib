journal = {Genome research},
keywords = {Animals,Base Sequence,Binding Sites,Binding Sites: genetics,Conserved Sequence,DNA,DNA Transposable Elements,DNA Transposable Elements: genetics,DNA: genetics,DNA: metabolism,Evolution, Molecular,Humans,Mice,Repetitive Sequences, Nucleic Acid,Sequence Homology, Nucleic Acid,Transcription Factors,Transcription Factors: metabolism},
month = nov,
number = {11},
pages = {1752--62},
pmid = {18682548},
title = {{Evolution of the mammalian transcription factor binding repertoire via transposable elements.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18682548},
volume = {18},
year = {2008}
}
