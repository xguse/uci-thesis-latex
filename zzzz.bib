keywords = {Animals,Culicidae,Culicidae: genetics,Evolution, Molecular,Gene Expression Profiling,Genomics,Multigene Family,Multigene Family: genetics,Phylogeny,Regulatory Sequences, Nucleic Acid},
month = mar,
number = {9},
pages = {3053--8},
pmid = {19211788},
title = {{Comparative genomics allows the discovery of cis-regulatory elements in mosquitoes.}},
url = {http://www.pnas.org/cgi/content/abstract/106/9/3053},
volume = {106},
year = {2009}
}
