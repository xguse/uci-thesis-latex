keywords = {Aedes,Aedes: chemistry,Aedes: classification,Aedes: enzymology,Aedes: genetics,Amino Acid Sequence,Animals,Digestive System,Digestive System: chemistry,Digestive System: enzymology,Gene Expression Profiling,Insect Proteins,Insect Proteins: chemistry,Insect Proteins: genetics,Insect Proteins: metabolism,Molecular Sequence Data,Phylogeny,Sequence Alignment,Serine Proteases,Serine Proteases: chemistry,Serine Proteases: genetics,Serine Proteases: metabolism},
month = jul,
