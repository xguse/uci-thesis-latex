issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Animals,Binding Sites,Computational Biology,Computational Biology: methods,Computer Graphics,Gene Expression Profiling,Mice,Oligonucleotide Array Sequence Analysis,Regulatory Elements, Transcriptional,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Software,Transcription Factors,Transcription Factors: metabolism},
