keywords = {Animals,Anopheles,Anopheles: enzymology,Anopheles: genetics,Anopheles: parasitology,Base Sequence,Blotting, Southern,Exons,Exons: genetics,Genes, Insect,Genes, Insect: genetics,Humans,In Situ Hybridization, Fluorescence,Introns,Introns: genetics,Microsatellite Repeats,Molecular Sequence Data,Nitric Oxide Synthase,Nitric Oxide Synthase Type II,Nitric Oxide Synthase: genetics,Plasmodium falciparum,Plasmodium falciparum: growth \& development,Polymorphism, Genetic,Sequence Analysis, DNA,Transcription, Genetic},
month = may,
