journal = {BMC bioinformatics},
keywords = {Algorithms,Binding Sites,Computational Biology,Computational Biology: methods,Data Interpretation, Statistical,Gene Expression Profiling,Gene Expression Regulation,Genome, Human,Humans,Oligonucleotide Array Sequence Analysis,Promoter Regions, Genetic,RNA, Messenger,RNA, Messenger: metabolism,Regulatory Elements, Transcriptional,Transcription Factors,Transcription Factors: chemistry,Transcription, Genetic},
month = jan,
pages = {330},
pmid = {16817975},
title = {{Genome-wide prediction of transcriptional regulatory elements of human promoters using gene expression and promoter analysis data.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1586028\&tool=pmcentrez\&rendertype=abstract},
