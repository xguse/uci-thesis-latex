doi = {10.1038/35000615},
issn = {0028-0836},
journal = {Nature},
keywords = {Animals,Bacterial Proteins,Base Sequence,Binding Sites,DNA,Drosophila Proteins,Drosophila melanogaster,Enhancer Elements, Genetic,Evolution, Molecular,Genes, Reporter,Homeodomain Proteins,Homeodomain Proteins: genetics,Molecular Sequence Data,Selection, Genetic,Species Specificity,Transcription Factors},
