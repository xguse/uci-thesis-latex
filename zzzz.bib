@article{Sirot2011,
abstract = {No commercially licensed vaccine or treatment is available for dengue fever, a potentially lethal infection that impacts millions of lives annually. New tools that target mosquito control may reduce vector populations and break the cycle of dengue transmission. Male mosquito seminal fluid proteins (Sfps) are one such target since these proteins, in aggregate, modulate the reproduction and feeding patterns of the dengue vector, Aedes aegypti. As an initial step in identifying new targets for dengue vector control, we sought to identify the suite of proteins that comprise the Ae. aegypti ejaculate and determine which are transferred to females during mating.},
