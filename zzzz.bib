month = dec,
number = {6},
pages = {589--97},
pmid = {17046229},
title = {{Thinking globally and acting locally with TOR.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17046229},
volume = {18},
year = {2006}
}
