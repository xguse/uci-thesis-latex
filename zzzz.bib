journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Aedes,Aedes: genetics,Animals,Animals, Genetically Modified,Carboxypeptidases,Carboxypeptidases: genetics,Gene Expression Regulation,Genes, Reporter,Humans,Promoter Regions, Genetic},
month = sep,
