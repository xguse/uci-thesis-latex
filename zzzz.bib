issn = {1471-2105},
journal = {BMC bioinformatics},
keywords = {Algorithms,Animals,Caenorhabditis elegans,Drosophila melanogaster,Mice,MicroRNAs,MicroRNAs: chemistry,Nucleic Acid Conformation,Nucleic Acid Heteroduplexes,Nucleic Acid Heteroduplexes: chemistry,RNA, Messenger,RNA, Messenger: chemistry,Software},
