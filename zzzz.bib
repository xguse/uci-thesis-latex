month = jun,
number = {3},
pages = {253--9},
pmid = {18436437},
title = {{The RNA polymerase II core promoter - the gateway to transcription.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18436437},
volume = {20},
