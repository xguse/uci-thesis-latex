issn = {1365-2583},
journal = {Insect molecular biology},
month = apr,
pmid = {21496127},
title = {{A role for endosomal proteins in alphavirus dissemination in mosquitoes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21496127},
year = {2011}
}
