issn = {1756-0500},
journal = {BMC research notes},
month = jan,
pages = {58},
pmid = {19374758},
title = {{Simultaneous tracking of movement and gene expression in multiple Drosophila melanogaster flies using GFP and DsRED fluorescent reporter transgenes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2679045\&tool=pmcentrez\&rendertype=abstract},
volume = {2},
year = {2009}
}
