keywords = {Animals,Arthropod Vectors,Arthropod Vectors: genetics,Blood-Borne Pathogens,Databases, Nucleic Acid,Evolution, Molecular,Expressed Sequence Tags,Genome, Insect,Genomics,Humans,Phylogeny,Sequence Analysis, DNA},
month = may,
