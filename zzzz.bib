month = jan,
pages = {65},
pmid = {20109229},
title = {{Haplotype allelic classes for detecting ongoing positive selection.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2831848\&tool=pmcentrez\&rendertype=abstract},
volume = {11},
